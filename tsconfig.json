{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "*"
      ]
    },
    "types": ["nativewind/types"]
  },
  "include": [
    "app/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "typedocOptions": {
  "entryPoints": [
    "index.ts",
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}"
  ],
    "out": "docs"
  }
}
