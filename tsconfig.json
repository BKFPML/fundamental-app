{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "index.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "typedocOptions": {
  "entryPoints": [
    "index.ts",
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "utils/*.{ts,tsx}",
    "Services/*.{ts,tsx}"
  ],
    "out": "docs"
  }
}
